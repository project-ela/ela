  if actual != expected {
    exit(1)
  }
}

func assertb(actual: bool, expected: bool) {
  if actual != expected {
    exit(1)
  }
}

func hoge(): int {
  return 1 * 2 + 3 * 4
}

func fuga(): int {
  return 3 * 4 + 1 * hoge()
}

func add(a: int, b: int): int {
  return a + b
}

func fib(n: int): int {
  if n <= 1 {
    return n
  } else {
    return fib(n - 1) + fib(n - 2)
  }
}

